generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Profile {
  id            String    @id @default(uuid())
  clerkId       String    @unique
  firstName     String
  lastName      String
  username      String
  email         String
  profileImage  String
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
}